<?xml version="1.0" encoding="utf-8"?>
<data-map xmlns="http://cayenne.apache.org/schema/3.0/modelMap"
	 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xsi:schemaLocation="http://cayenne.apache.org/schema/3.0/modelMap http://cayenne.apache.org/schema/3.0/modelMap.xsd"
	 project-version="6">
	<property name="defaultPackage" value="com.ade.fun.model.persistent"/>
	<db-entity name="T_ACCOUNT">
		<db-attribute name="ACCOUNT_CODE" type="VARCHAR" length="20"/>
		<db-attribute name="ACCOUNT_DESC" type="VARCHAR" length="200"/>
		<db-attribute name="ACCOUNT_ID" type="INTEGER" isPrimaryKey="true" isMandatory="true" length="9"/>
		<db-attribute name="ACCOUNT_TYPE" type="INTEGER" length="1"/>
	</db-entity>
	<db-entity name="T_CHARACTER">
		<db-attribute name="ACCOUNT_ID" type="INTEGER" length="9"/>
		<db-attribute name="CHARACTER_ID" type="INTEGER" isPrimaryKey="true" isMandatory="true" length="9"/>
		<db-attribute name="CHARACTER_LEVEL" type="INTEGER" length="3"/>
		<db-attribute name="CHARACTER_POSITION" type="INTEGER" length="1"/>
		<db-attribute name="CHARACTER_RANK" type="INTEGER" length="1"/>
		<db-attribute name="OCCUPATION_ID" type="INTEGER" length="9"/>
	</db-entity>
	<db-entity name="T_OCCUPATION">
		<db-attribute name="OCCUPATION_CATEGORY_ID" type="INTEGER" length="9"/>
		<db-attribute name="OCCUPATION_ID" type="INTEGER" isPrimaryKey="true" isMandatory="true" length="9"/>
		<db-attribute name="OCCUPATION_NAME" type="VARCHAR" length="20"/>
	</db-entity>
	<db-entity name="T_OCCUPATION_CATEGORY">
		<db-attribute name="OCCUPATION_CATEGORY_DESC" type="VARCHAR" length="200"/>
		<db-attribute name="OCCUPATION_CATEGORY_ID" type="INTEGER" isPrimaryKey="true" isMandatory="true" length="9"/>
		<db-attribute name="OCCUPATION_CATEGORY_NAME" type="VARCHAR" length="20"/>
	</db-entity>
	<obj-entity name="Account" className="com.ade.fun.model.persistent.Account" dbEntityName="T_ACCOUNT">
		<obj-attribute name="accountCode" type="java.lang.String" db-attribute-path="ACCOUNT_CODE"/>
		<obj-attribute name="accountDesc" type="java.lang.String" db-attribute-path="ACCOUNT_DESC"/>
		<obj-attribute name="accountType" type="java.lang.Integer" db-attribute-path="ACCOUNT_TYPE"/>
	</obj-entity>
	<obj-entity name="Character" className="com.ade.fun.model.persistent.Character" dbEntityName="T_CHARACTER">
		<obj-attribute name="characterLevel" type="java.lang.Integer" db-attribute-path="CHARACTER_LEVEL"/>
		<obj-attribute name="characterPosition" type="com.ade.fun.model.enumeration.Position" db-attribute-path="CHARACTER_POSITION"/>
		<obj-attribute name="characterRank" type="com.ade.fun.model.enumeration.Rank" db-attribute-path="CHARACTER_RANK"/>
	</obj-entity>
	<obj-entity name="Occupation" className="com.ade.fun.model.persistent.Occupation" dbEntityName="T_OCCUPATION">
		<obj-attribute name="occupationName" type="java.lang.String" db-attribute-path="OCCUPATION_NAME"/>
	</obj-entity>
	<obj-entity name="OccupationCategory" className="com.ade.fun.model.persistent.OccupationCategory" dbEntityName="T_OCCUPATION_CATEGORY">
		<obj-attribute name="occupationCategoryDesc" type="java.lang.String" db-attribute-path="OCCUPATION_CATEGORY_DESC"/>
		<obj-attribute name="occupationCategoryName" type="java.lang.String" db-attribute-path="OCCUPATION_CATEGORY_NAME"/>
	</obj-entity>
	<db-relationship name="Character" source="T_ACCOUNT" target="T_CHARACTER" toMany="true">
		<db-attribute-pair source="ACCOUNT_ID" target="ACCOUNT_ID"/>
	</db-relationship>
	<db-relationship name="Account" source="T_CHARACTER" target="T_ACCOUNT" toMany="false">
		<db-attribute-pair source="ACCOUNT_ID" target="ACCOUNT_ID"/>
	</db-relationship>
	<db-relationship name="Occupation" source="T_CHARACTER" target="T_OCCUPATION" toMany="false">
		<db-attribute-pair source="OCCUPATION_ID" target="OCCUPATION_ID"/>
	</db-relationship>
	<db-relationship name="Character" source="T_OCCUPATION" target="T_CHARACTER" toMany="true">
		<db-attribute-pair source="OCCUPATION_ID" target="OCCUPATION_ID"/>
	</db-relationship>
	<db-relationship name="OccupationCategory" source="T_OCCUPATION" target="T_OCCUPATION_CATEGORY" toMany="false">
		<db-attribute-pair source="OCCUPATION_CATEGORY_ID" target="OCCUPATION_CATEGORY_ID"/>
	</db-relationship>
	<db-relationship name="Occupation" source="T_OCCUPATION_CATEGORY" target="T_OCCUPATION" toMany="true">
		<db-attribute-pair source="OCCUPATION_CATEGORY_ID" target="OCCUPATION_CATEGORY_ID"/>
	</db-relationship>
	<obj-relationship name="character" source="Account" target="Character" deleteRule="Deny" db-relationship-path="Character"/>
	<obj-relationship name="account" source="Character" target="Account" deleteRule="Nullify" db-relationship-path="Account"/>
	<obj-relationship name="occupation" source="Character" target="Occupation" deleteRule="Nullify" db-relationship-path="Occupation"/>
	<obj-relationship name="character" source="Occupation" target="Character" deleteRule="Deny" db-relationship-path="Character"/>
	<obj-relationship name="occupationCategory" source="Occupation" target="OccupationCategory" deleteRule="Nullify" db-relationship-path="OccupationCategory"/>
	<obj-relationship name="occupation" source="OccupationCategory" target="Occupation" deleteRule="Deny" db-relationship-path="Occupation"/>
</data-map>
